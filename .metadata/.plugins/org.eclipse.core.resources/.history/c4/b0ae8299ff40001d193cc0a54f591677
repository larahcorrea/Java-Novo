package ex54;

public class Pessoa {
	public int id;
	public String nome;
	public Dependente dependente;
	
	
	
	Pessoa(){
		
	}
	Pessoa (int id, String nome, Dependente dep){
		this.id = id;
		this.nome = nome;
		this.dependente = dep;
		
	}
	
	public void transformaNomeMaiusculo() {
		this.nome = this.nome.toUpperCase();
		
	}
	
	public String exibirNome() {
		return this.nome = (this.id + " - " + this.nome);
	}
	
	
	

}
