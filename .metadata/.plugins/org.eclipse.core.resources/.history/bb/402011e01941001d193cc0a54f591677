package Ex8;

import java.io.ObjectInputFilter.Config;

import ex54.Dependente;

public class Cliente {
	
	public int id;
	public String nome;
	public int idade;
	public String email;
	public ContaBancaria ContaBancaria;

	
	
	Cliente(){
		
	}
	 
	Cliente (int id, String nome, int idade,String email, ContaBancaria cb){
		this.id = id;
		this.nome = nome;
		this.email = email;
		this.ContaBancaria = cb;
	}
	public void atualizarEmail(String novoEmail) {
		this.email = novoEmail;
	}
	public String exibirDadosConta() {
		return this("Id: "this.id + " - " "Nome: "+ this.nome + " - ""Email: " this.email);
	}
	
	public String exibirDadosBanco() {
		return this("Agencia: "this.agencia + " - " "NÃºmero: "+ this.numero + " - ""Saldo: " this.saldo);
	

}

}